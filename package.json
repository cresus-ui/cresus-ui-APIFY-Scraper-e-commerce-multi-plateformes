{
  "name": "multi-platform-ecommerce-scraper",
  "version": "1.0.0",
  "description": "Scraper e-commerce multi-plateformes pour Amazon, eBay, Walmart, Etsy, Shopify avec suivi automatique des prix, stocks et tendances",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "nodemon src/main.js",
    "build": "npm run lint && npm run test",
    "docker:build": "docker build -t ecommerce-scraper .",
    "docker:run": "docker run -it ecommerce-scraper"
  },
  "keywords": [
    "apify",
    "scraper",
    "ecommerce",
    "amazon",
    "ebay",
    "walmart",
    "etsy",
    "shopify",
    "price-tracking",
    "stock-monitoring"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "apify": "^3.1.10",
    "playwright": "^1.40.0",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.6.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "user-agents": "^1.0.1235"
  },
  "devDependencies": {
    "@apify/eslint-config": "^0.4.0",
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.0",
    "eslint-plugin-jest": "^27.2.0",
    "jest": "^29.5.0",
    "jest-html-reporters": "^3.1.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.0"
  },
  "repository": {
    "type": "git",
    "url": "."
  }
}